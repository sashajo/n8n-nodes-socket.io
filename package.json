{
  "name": "@sashajo/n8n-nodes-socketio",
  "version": "0.1.9",
  "description": "Listen to Socket.IO events and trigger workflows",
  "keywords": [
    "n8n-community-node-package",
    "socket.io",
    "socket.io-client"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Sasha Jovicic",
    "email": "sasha.jo@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sashajo/n8n-nodes-socket.io.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SocketAuthApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SocketIoNode/SocketIoTriggerNode.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "socket.io-client": "^4.8.1"
  },
  "publishConfig": {
    "access": "public"
  }
}